{
	"manifest_version": 2,
	"name": "Minimal New Tab",
	"version": "1.6",
	"description": "newtab",
	"chrome_url_overrides": {
		"newtab": "index.html"
	},
	"incognito": "split",
	"permissions": [
		"activeTab",
		"topSites",
		"*://suggestqueries.google.com/complete/*",
		"https://t2.gstatic.com/faviconV2?client=SOCIAL&type=FAVICON&fallback_opts=TYPE,SIZE,URL&url=*"
	]
}