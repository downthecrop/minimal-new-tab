{
	"manifest_version": 2,
	"name": "Minimal New Tab",
	"version": "1.2",
	"description": "newtab",
	"chrome_url_overrides": {
		"newtab": "index.html"
	},
	"incognito": "split",
	"permissions": [
		"activeTab",
		"topSites",
		"*://suggestqueries.google.com/complete/*",
		"https://www.google.com/s2/favicons?domain="
	]
}